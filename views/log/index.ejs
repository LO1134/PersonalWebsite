<%- include('../includes/head.ejs') %>
    <link rel="stylesheet" href="/css/log.css">
</head>

<body>
    <%- include('../includes/navigation.ejs') %>

    <main>
          <% if (entries.length > 0) { %>
            <div class="">
                <table id='sitelog'>
                    <tr>
                        <th>Date</th>
                        <th>CalledBy</th>
                        <th>Message</th>
                    </tr>
                    <% for (let log of entries) { %>                       
                    <tr>
                        <td width='20%'><%= log.Date %></td>
                        <td width='17%'><%= log.CalledBy %></td>
                        <td width='60%'><%= log.Message %></td>
                    </tr>                        
                   <% } %>
                </table>
            </div>

        <% } else { %>
            <h1>No Entries in log Found!</h1>
        <% } %>   
    </main>
<%- include('../includes/end.ejs') %>