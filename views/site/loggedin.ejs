<%- include('../includes/head.ejs') %>
    <link rel="stylesheet" href="/css/site.css">
</head>

<body>
    <%- include('../includes/navigation.ejs') %>

    <main>
          <% if (entries.length > 0) { %>
            <div class="">
                <table id='inloggers'>
                    <tr>
                        <th>loginName</th>
                        <th>loginTime</th>
                        <th>Gebruikte password</th>
                        <th>1 is gelukt</th></th>
                        <th>IP address</th></th>
                    </tr>
                    <% for (let entry of entries) { %>                       
                    <tr>
                        <td width='2%'><%= entry.loginName %></td>
                        <td width='17%'><%= entry.loginTime %></td>
                        <td width='2%'><%= entry.password %></td>
                        <td width='1%'><%= entry.result %></td>
                        <td width='15%'><%= entry.IPAddress %></td>				
                    </tr>                        
                   <% } %>
                </table>
            </div>

        <% } else { %>
            <h1>No Entries in logtab Found!</h1>
        <% } %>   
    </main>
<%- include('../includes/end.ejs') %>